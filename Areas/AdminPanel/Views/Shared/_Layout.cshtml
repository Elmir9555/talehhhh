<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
    <meta name="description" content="" />
    <meta name="author" content="" />
    <title>Dashtreme Admin - Free Dashboard for Bootstrap 4 by Codervent</title>
    <!-- loader-->
    <link href="~/AdminPanel/assets/css/pace.min.css" rel="stylesheet" />
    <script src="~/AdminPanel/assets/js/pace.min.js"></script>
    <!--favicon-->
    <link rel="icon" href="~/AdminPanel/assets/images/favicon.ico" type="image/x-icon">
    <!-- Vector CSS -->
    <link href="~/AdminPanel/assets/plugins/vectormap/jquery-jvectormap-2.0.2.css" rel="stylesheet" />
    <!-- simplebar CSS-->
    <link href="~/AdminPanel/assets/plugins/simplebar/css/simplebar.css" rel="stylesheet" />
    <!-- Bootstrap core CSS-->
    <link href="~/AdminPanel/assets/css/bootstrap.min.css" rel="stylesheet" />
    <!-- animate CSS-->
    <link href="~/AdminPanel/assets/css/animate.css" rel="stylesheet" type="text/css" />
    <!-- Icons CSS-->
    <link href="~/AdminPanel/assets/css/icons.css" rel="stylesheet" type="text/css" />
    <!-- Sidebar CSS-->
    <link href="~/AdminPanel/assets/css/sidebar-menu.css" rel="stylesheet" />
    <!-- Custom Style-->
    <link href="~/AdminPanel/assets/css/app-style.css" rel="stylesheet" />

    <link href="~/AdminPanel/assets/css/view.css" rel="stylesheet"/>

    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.8.3/font/bootstrap-icons.css">
</head>

<body class="bg-theme bg-theme1">

    <!-- Start wrapper-->
    <div id="wrapper">

        <!--Start sidebar-wrapper-->
        <div id="sidebar-wrapper" data-simplebar="" data-simplebar-auto-hide="true">
            <div class="brand-logo">
                <a href="~/AdminPanel/index.html">
                    <img src="~/AdminPanel/assets/images/logo-icon.png" class="logo-icon" alt="logo icon">
                    <h5 class="logo-text">Dashtreme Admin</h5>
                </a>
            </div>
            <ul class="sidebar-menu do-nicescrol">
                <li class="sidebar-header">Base Tables</li>

                <li>
                    <a asp-action="CategoryList" asp-controller="Category" asp-area="AdminPanel">
                        <i class="zmdi zmdi-view-dashboard"></i> <span>Category</span>
                    </a>
                </li>

                <li>
                    <a asp-action="ProductList" asp-controller="Product" asp-area="AdminPanel">
                        <i class="zmdi zmdi-grid"></i> <span>Product</span>
                    </a>
                </li>

                <li>
                    <a asp-action="ProductDetailsList" asp-controller="ProductDetails" asp-area="AdminPanel">
                        <i class="zmdi zmdi-face"></i> <span>ProductDetails</span>
                    </a>
                </li>

                <li>
                    <a asp-action="SaleOffList" asp-controller="SaleOff" asp-area="AdminPanel">
                        <i class="zmdi zmdi-account-circle"></i> <span>SaleOff</span>
                    </a>
                </li>

                <li>
                    <a asp-action="SaleOffDetailsList" asp-controller="SaleOffDetails" asp-area="AdminPanel">
                        <i class="zmdi zmdi-lock"></i> <span>SaleOffDetails</span>
                    </a>
                </li>

                <li>
                    <a asp-action="BlogList" asp-controller="Blog" asp-area="AdminPanel">
                        <i class="zmdi zmdi-grid"></i> <span>Blog</span>
                    </a>
                </li>

                <li>
                    <a asp-action="BlogDetailsList" asp-controller="BlogDetails" asp-area="AdminPanel">
                        <i class="zmdi zmdi-face"></i> <span>BlogDetails</span>
                    </a>
                </li>

                <li class="sidebar-header">Other Tables</li>
                <li>
                    <a asp-action="CommentList" asp-controller="Comment" asp-area="AdminPanel">
                        <i class="zmdi zmdi-format-list-bulleted"></i> <span>Comment</span>
                    </a>
                </li>
                <li>
                    <a asp-action="OwnerList" asp-controller="Owner" asp-area="AdminPanel">
                        <i class="zmdi zmdi-share text-info"></i> <span>Owner</span>
                    </a>
                </li>
                <li>
                    <a asp-action="AdvertList" asp-controller="Advert" asp-area="AdminPanel">
                        <i class="zmdi zmdi-coffee text-danger"></i> <span>Advert</span>
                    </a>
                </li>
                <li>
                    <a asp-action="ContactList" asp-controller="Contact" asp-area="AdminPanel">
                        <i class="zmdi zmdi-chart-donut text-success"></i> <span>Contact</span>
                    </a>
                </li>
                <li>
                    <a asp-action="SubcribeList" asp-controller="Subcribe" asp-area="AdminPanel">
                        <i class="zmdi zmdi-format-list-bulleted"></i> <span>Subcribe</span>
                    </a>
                </li>

            </ul>

        </div>
        <!--End sidebar-wrapper-->

        <!--Start topbar header-->
        <header class="topbar-nav">
            <nav class="navbar navbar-expand fixed-top">
                <ul class="navbar-nav mr-auto align-items-center">
                    <li class="nav-item">
                        <a class="nav-link toggle-menu" href="~/AdminPanel/javascript:void();">
                            <i class="icon-menu menu-icon"></i>
                        </a>
                    </li>
                    
                </ul>

                <ul class="navbar-nav align-items-center right-nav-link">

                    <li class="nav-item">
                        <a class="nav-link dropdown-toggle dropdown-toggle-nocaret" data-toggle="dropdown" href="~/AdminPanel/#">
                            <span class="user-profile"><img src="~/AdminPanel/https://via.placeholder.com/110x110" class="img-circle" alt="user avatar"></span>
                        </a>
                        <ul class="dropdown-menu dropdown-menu-right">
                            <li class="dropdown-item user-details">
                                <a href="~/AdminPanel/javaScript:void();">
                                    <div class="media">
                                        <div class="avatar"><img class="align-self-start mr-3" src="https://via.placeholder.com/110x110" alt="Admin Panel"></div>
                                        <div class="media-body">
                                            <h6 class="mt-2 user-title">Taleh Suleymanov</h6>
                                            <p class="user-subtitle">talehsuleymanov887@gmail.com</p>
                                        </div>
                                    </div>
                                </a>
                            </li>
                            <li class="dropdown-divider"></li>
                            <li class="dropdown-item"><i class="icon-envelope mr-2"></i> Inbox</li>
                            <li class="dropdown-divider"></li>
                            <li class="dropdown-item"><i class="icon-wallet mr-2"></i> Account</li>
                            <li class="dropdown-divider"></li>
                            <li class="dropdown-item"><i class="icon-settings mr-2"></i> Setting</li>
                            <li class="dropdown-divider"></li>
                            <li class="dropdown-item"><i class="icon-power mr-2"></i> Logout</li>
                        </ul>
                    </li>
                </ul>
            </nav>
        </header>
        <!--End topbar header-->


        <div class="clearfix"></div>

        <div class="content-wrapper">
            <div class="container-fluid">


                @RenderBody()


            </div>


        </div>


        
        <!--start color switcher-->
        <div class="right-sidebar">
            <div class="switcher-icon">
                <i class="zmdi zmdi-settings zmdi-hc-spin"></i>
            </div>
            <div class="right-sidebar-content">

                <p class="mb-0">Gaussion Texture</p>
                <hr>

                <ul class="switcher">
                    <li id="theme1"></li>
                    <li id="theme2"></li>
                    <li id="theme3"></li>
                    <li id="theme4"></li>
                    <li id="theme5"></li>
                    <li id="theme6"></li>
                </ul>

                <p class="mb-0">Gradient Background</p>
                <hr>

                <ul class="switcher">
                    <li id="theme7"></li>
                    <li id="theme8"></li>
                    <li id="theme9"></li>
                    <li id="theme10"></li>
                    <li id="theme11"></li>
                    <li id="theme12"></li>
                    <li id="theme13"></li>
                    <li id="theme14"></li>
                    <li id="theme15"></li>
                </ul>

            </div>
        </div>
        <!--end color switcher-->

    </div><!--End wrapper-->
    <!-- Bootstrap core JavaScript-->
    <script src="~/AdminPanel/assets/js/jquery.min.js"></script>
    <script src="~/AdminPanel/assets/js/popper.min.js"></script>
    <script src="~/AdminPanel/assets/js/bootstrap.min.js"></script>

    <!-- simplebar js -->
    <script src="~/AdminPanel/assets/plugins/simplebar/js/simplebar.js"></script>
    <!-- sidebar-menu js -->
    <script src="~/AdminPanel/assets/js/sidebar-menu.js"></script>
    <!-- loader scripts -->
    <script src="~/AdminPanel/assets/js/jquery.loading-indicator.js"></script>
    <!-- Custom scripts -->
    <script src="~/AdminPanel/assets/js/app-script.js"></script>
    <!-- Chart js -->

    <script src="~/AdminPanel/assets/plugins/Chart.js/Chart.min.js"></script>

    <!-- Index js -->
    <script src="~/AdminPanel/assets/js/index.js"></script>

</body>
</html>
